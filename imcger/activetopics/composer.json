{
    "name": "imcger/activetopics",
    "type": "phpbb-extension",
    "description": "This extension enables settings for active topics.",
    "homepage": "https://github.com/IMC-GER/phpBB-Active-Topics/",
    "version": "1.1.1",
    "time": "2024-10-13",
    "keywords": ["phpbb", "extension", "activetopics"],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Thorsten Ahlers",
            "homepage": "https://github.com/IMC-GER",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "phpbb/phpbb": "^3.3.0",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Active Topics",
        "soft-require": {
            "phpbb/phpbb": "^3.3.0"
        },
        "version-check": {
            "host": "raw.githubusercontent.com",
            "directory": "/IMC-GER/phpBB-Active-Topics/master",
            "filename": "activetopics_version.json",
            "ssl": true
        }
    }
}
